@Article{	  waleffe.f:exact,
  author	= "Waleffe, Fabian",
  title		= "Exact coherent structures in channel flow",
  language	= "English",
  journal	= "J. Fluid Mech. ",
  volume	= "435",
  pages		= "93-102",
  year		= "2001",
  abstract	= "{Summary: Exact coherent states in no-slip plane
		  Poiseuille flow are calculated by homotopy from free-slip
		  to no-slip boundary conditions. These coherent states are
		  unstable traveling waves. They consist of wavy low-speed
		  streaks flanked by staggered streamwise vortices closely
		  resembling the asymmetric coherent structures observed in
		  the near-wall region of turbulent flows. We show that the
		  traveling waves arise from a saddle-node bifurcation at a
		  sub-turbulent Reynolds number with wall-normal, spanwise
		  and streamwise dimensions smaller than but comparable to
		  $50^+$, $100^+$ and $250^+$, respectively. These coherent
		  solutions come in pairs with distinct structure and
		  instabilities, and there is a three-dimensional continuum
		  of such exact coherent states.}",
  keywords	= "{no-slip plane Poiseuille flow; homotopy; wavy low-speed
		  streak; staggered streamwise vortices; coherent structures;
		  saddle-node bifurcation; sub-turbulent Reynolds number}",
  classmath	= "{*76F06 (Transition to turbulence) 76D05 (Navier-Stokes
		  equations (fluid dynamics)) 76M22 (Spectral methods) }"
}

