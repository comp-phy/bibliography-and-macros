@Article{	  schlichting.h:zur,
  author	= "Schlichting, H.",
  title		= {Zur Entstehung der Turbulenz bei der Plattenstr\"omung},
  language	= "German",
  year		= 1933,
  abstract	= "{Bei der Stabilit\"atsuntersuchung einer laminaren
		  Str\"omung nach der Methode der kleinen Schwingungen denkt
		  man sich der vorgegebenen Str\"omung kleine St\"orungen in
		  Form einer in der Hauptstr\"omungsrichtung fortschreitenden
		  Wellenbewegung \"uberlagert. Die St\"orungsbewegung wird
		  nach {\it Fourier} in Partialschwingungen zerlegt; f\"ur
		  die St\"orungsamplituden erh\"alt man dann aus den {\it
		  Navier-Stokes}schen Bewegungsgleichungen die
		  Str\"omungsdifferentialgleichung. Die Ermittlung der
		  Anfachung einer gegebenen St\"orung erweist sich als ein
		  Eigenwertproblem dieser Gleichung, derart, da\ss {} man zu
		  jedem Paar von Werten f\"ur die r\"aumliche Kreisfrequenz
		  der St\"orung und der {\it Reynolds}schen Zahl einen Wert
		  $\beta _i$ erhalten kann, dessen Imagin\"arteil \"uber die
		  Anfachung oder D\"ampfung der betreffenden St\"orung
		  entscheidet. Fr\"uher hat {\it Tollmien} (1929; F. d. M.
		  $55_{\text {I}}$, 474) dieses Problem f\"ur den Spezialfall
		  $\beta _i = 0$ gel\"ost und dadurch diejenigen St\"orungen
		  erhalten, die gerade an der Grenze zwischen Stabilit\"at
		  und Labilit\"at liegen. Es zeigt sich, da\ss {} die
		  Wellenl\"ange der instabilen St\"orungen etwa von der
		  Gr\"o\ss enordnung der 30-fachen Verdr\"angungsdicke der
		  Grenzschicht sind. Durch Fortbildung dieser Rechnungen
		  konnte n\"aherungsweise die Anfachung der instabilen
		  St\"orungen berechnet werden. Dadurch wurde eine
		  Kurvenschar $\beta _i = \text {const}$ erhalten. Verfolgt
		  man eine St\"orung auf ihrem Wege l\"angs der Platte, so
		  durchl\"auft sie, indem sie sich mit der
		  Gruppengeschwindigkeit fortpflanzt, zun\"achst einen
		  stabilen Bereich, tritt dann mit einer bestimmten Amplitude
		  in den instabilen Bereich ein, den sie mit einer andern
		  Amplitude wieder verl\"a\ss t. Das ma\ss gebende Ma\ss {}
		  f\"ur die Anfachung ist deren gr\"o\ss tm\"oglicher Wert,
		  unter Zulassung aller m\"oglichen Frequenzen der
		  St\"orungen; f\"ur diesen wird eine einfache
		  Interpolationsformel gegeben. Die beobachteten Werte f\"ur
		  die Stabilit\"atsgrenze liegen bei Anfachungen, die
		  durchaus aus den langen instabilen St\"orungswellenl\"angen
		  erkl\"art werden kann.}",
  reviewer	= "{P\"oschl, Th.; Prof. (Karlsruhe)}",
  journal	= {Nachrichten von der Gesellschaft der Wissenschaften zu
		  G\"ottingen},
  pages		= {181--208}
}

@Article{	  schmid.pj.henningson.ds:optimal,
  author	= "Schmid, Peter J. and Henningson, Dan S.",
  title		= "Optimal energy density growth in {H}agen-{P}oiseuille
		  flow",
  language	= "English",
  journal	= "J. Fluid Mech.",
  volume	= "277",
  pages		= "197-225",
  year		= "1994",
  abstract	= "{The paper analyses the linear initial value problem for
		  the evolution of infinitesimal disturbances in
		  incompressible circular pipe flow. Both asymptotic and
		  transient behavior are studied. Since the corresponding
		  linear operator is non-normal, its eigenvalues govern only
		  the asymptotic stability. To investigate the transient
		  growth and conditions for the occurence of transient
		  effects, the $\varepsilon$-pseudospectra are analysed. It
		  is shown that the flow field of the optimal disturbance is
		  characterised by a strong shear layer at the centre of the
		  pipe, and its overall structure does not change
		  significantly with time.}",
  reviewer	= "{Iuliana Oprea (Bucure\c{s}ti)}",
  keywords	= "{non-normal operator; linear initial value problem;
		  incompressible circular pipe flow; eigenvalues; asymptotic
		  stability; transient growth; optimal disturbance;
		  epsilon-spectrum}",
  classmath	= "{*76E05 (Stability of parallel flows) 76M25 (Other
		  numerical methods) }"
}

