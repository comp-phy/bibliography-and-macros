@Article{	  slemrod.m:constitutive,
  author	= "Slemrod, M.",
  title		= "Constitutive relations for {R}ivlin--{E}ricksen fluids
		  based on generalized rational approximation",
  language	= "English",
  journal	= "Arch. Ration. Mech. Anal. ",
  volume	= "146",
  number	= "1",
  pages		= "73-93",
  year		= "1999",
  doi		= {10.1007/s002050050137},
  abstract	= "{In an incompressible Rivlin-Ericksen liquid, stress
		  contains the spherical part (pressure component) plus some
		  polynomial function of tensors ${\bbfA}_1, {\bbfA}_2,
		  \dots\ $. Here ${\bbfA}_1$ is the (symmetric) velocity
		  deformation tensor, and the following symmetric tensors are
		  constructed recursively. Specifically, the second-order
		  Rivlin-Ericksen fluid has the extra stress of the following
		  form $\mu{\bbfA}_1+a_1{\bbfA}_2+a_2{\bbfA}_1^2$ (the former
		  term is the Newtonian viscous stress). Many rheologists
		  believe that $a_1&lt;0$ in polymeric fluids. The present
		  paper shows that the requirement $a_1&lt;0$ causes
		  instability of the rest state of the second-order fluid.
		  The author argues that approximation of the extra stress
		  via a simple rational function eliminates this
		  instability.}",
  reviewer	= "{P.Krapivsky (Boston)}",
  keywords	= "{incompressible Rivlin-Ericksen liquid; instability of
		  rest state of second-order fluid; approximation of extra
		  stress; rational function}",
  classmath	= "{*76A05 (Non-Newtonian fluids) 76E99 (Hydrodynamic
		  stability) }"
}

@Article{	  sohr.h.wahl.w:on,
  author	= "Sohr, Hermann and von Wahl, Wolf",
  title		= "On the regularity of the pressure of weak solutions of
		  {N}avier--{S}tokes equations",
  language	= "English",
  journal	= "Arch. Math.",
  volume	= "46",
  pages		= "428-439",
  year		= "1986",
  doi		= {10.1007/BF01210782},
  abstract	= "{The aim of this paper is to prove the regularity property
		  $p\in L\sp{5/3}$ for the pressure of weak solutions of
