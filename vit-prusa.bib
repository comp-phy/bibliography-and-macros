@Article{	  sivashinsky.gi:nonlinear,
  title		= "Nonlinear analysis of hydrodynamic instability in laminar
		  flames {I}. {D}erivation of basic equations",
  journal	= "Acta Astronautica",
  volume	= "4",
  number	= "11--12",
  pages		= "1177--1206",
  year		= "1977",
  note		= "",
  doi		= "10.1016/0094-5765(77)90096-0",
  author	= "Sivashinsky, G. I.",
  abstract	= "An asymptotic nonlinear integrodifferential equation is
		  derived for spontaneous instability of the plane front of a
		  laminar flame. If the combustible mixture is deficient in
		  the light component, spontaneous instability will lead to
		  self-turbulization of the flame, and the flame front
		  assumes a strongly nonstationary cellular structure. If
		  there is an excess of the light component, spontaneous
		  instability produces stationary, irregular wrinkles on the
		  flame front, and the flame continues to propagate in a
		  laminar regime. It is shown that in all cases spontaneous
		  instability of the flame implies an increase in its
		  propagation velocity. "
}

