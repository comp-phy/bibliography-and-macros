@article{slemrod.m:constitutive,
abstract = {In an incompressible Rivlin-Ericksen liquid, stress
contains the spherical part (pressure component) plus some
polynomial function of tensors \{$\backslash$bbfA\}\_1, \{$\backslash$bbfA\}\_2,
$\backslash$dots$\backslash$ . Here \{$\backslash$bbfA\}\_1 is the (symmetric) velocity
deformation tensor, and the following symmetric tensors are
constructed recursively. Specifically, the second-order
Rivlin-Ericksen fluid has the extra stress of the following
form $\backslash$mu\{$\backslash$bbfA\}\_1+a\_1\{$\backslash$bbfA\}\_2+a\_2\{$\backslash$bbfA\}\_1\^{}2 (the former
term is the Newtonian viscous stress). Many rheologists
believe that a\_1\&lt;0 in polymeric fluids. The present
paper shows that the requirement a\_1\&lt;0 causes
instability of the rest state of the second-order fluid.
The author argues that approximation of the extra stress
via a simple rational function eliminates this
instability.},
author = {Slemrod, M},
doi = {10.1007/s002050050137},
journal = {Arch. Ration. Mech. Anal.},
keywords = {incompressible Rivlin-Ericksen liquid; instability of rest state of second-order fluid; approximation of extra stress; rational function},
number = {1},
pages = {73--93},
title = {{Constitutive relations for \{R\}ivlin--\{E\}ricksen fluids based on generalized rational approximation}},
volume = {146},
year = {1999}
}
