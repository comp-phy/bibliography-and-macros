@article{funada.t.joseph.dd:viscous,
abstract = {From the summary: We study the stability of stratified
gas-liquid flow in a horizontal rectangular channel using
viscous potential flow. The analysis leads to an explicit
dispersion relation in which the effects of surface tension
and viscosity on the normal stress are not neglected, but
the effect of shear stresses is. Formulas for the growth
rates, wave speeds and neutral stability curve are given in
general, and are applied to experiments in air-water flows.
The effects of surface tension are always important and
determine the stability limits for the cases in which the
volume fraction of gas is not too small. The stability
criterion for viscous potential flow is expressed by a
critical value of relative velocity. The maximum critical
value is when the viscosity ratio is equal to the density
ratio; surprisingly, the neutral curve for this viscous
fluid is the same as the neutral curve for inviscid
fluids.},
author = {Funada, T and Joseph, D D},
doi = {10.1017/S0022112001005572},
journal = {J. Fluid Mech.},
keywords = {stratified gas-liquid flow; dispersion relation; surface tension; stability criterion},
pages = {263--283},
title = {{Viscous potential flow analysis of \{K\}elvin--\{H\}elmholtz instability in a channel}},
volume = {445},
year = {2001}
}
