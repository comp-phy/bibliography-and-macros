@article{blennerhassett.pj.bassom.ap:linear,
abstract = {Summary: We investigate the linear stability of Stokes
layers generated between a pair of synchronously
oscillating parallel plates. The disturbance equations are
studied using Floquet theory, and pseudospectral numerical
methods are used to solve the resulting system. Neutral
curves for an extensive range of plate separations are
obtained, and, when the plate separation is large compared
to the Stokes layer thickness, the linear stability
properties of the Stokes layer in a semi-infinite fluid are
recovered. A detailed analysis of the damping rates of
disturbances to the basic flow provides a plausible
explanation of why several previous studies of the problem
have failed to detect any linear instability of the flow.

To compare more faithfully with experimental work, the
techniques used for the channel problem are modified to
allow the determination of neutral curves for axisymmetric
disturbances to purely oscillatory flow in a circular pipe.
Critical Reynolds numbers for the pipe flow tend to be
smaller than their counterparts for the channel case, but
the smallest critical value is still almost twice the
experimentally reported result.},
author = {Blennerhassett, P J and Bassom, Andrew P},
journal = {J. Fluid Mech.},
keywords = {Floquet theory; pseudospectral methods; Stokes layer; neutral curves},
pages = {1--25},
title = {{The linear stability of high-frequency oscillatory flow in a channel.}},
volume = {556},
year = {2006}
}
