journal = {Mathematical biosciences},
keywords = {Algorithms,Computer Simulation,Electric Capacitance,Electric Conductivity,Electrochemistry,Electrophysiology,Heart,Heart: physiology,Humans,Models, Cardiovascular,Myocytes, Cardiac,Myocytes, Cardiac: physiology},
