@article{sohr.h.wahl.w:on,
abstract = {The aim of this paper is to prove the regularity property
p$\backslash$in L$\backslash$sp\{5/3\} for the pressure of weak solutions of
Navier-Stokes equations in a bounded or an exterior domain.
This result was known only for the whole space. The method
to prove this property uses a new potential theoretical
estimate of the linearized equation with different
integration exponents in space and time. Using this
regularity property of the pressure, it is possible to
prove the existence of a weak solution of Navier-Stokes
equations which is smooth for large $\backslash$vert x$\backslash$vert in an
exterior domain. This result has been proved by \{$\backslash$it L.
Caffarelli\}, \{$\backslash$it R. Kohn\} and \{$\backslash$it L.Nirenberg\} [Commun.
Pure Appl. Math. 35, 771-831 (1982; Zbl 0509.35067)] for
the whole space.},
author = {Sohr, Hermann and von Wahl, Wolf},
doi = {10.1007/BF01210782},
journal = {Arch. Math.},
keywords = {regularity; pressure of weak solutions; Navier-Stokes equations; linearized equation; existence; weak solution},
pages = {428--439},
title = {{On the regularity of the pressure of weak solutions of \{N\}avier--\{S\}tokes equations}},
volume = {46},
